@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix game: <http://example.org/game-ontology#> .

# Ontology declaration
<http://example.org/game-ontology> rdf:type owl:Ontology ;
    rdfs:label "Game Ontology" ;
    rdfs:comment "An ontology describing video games, their properties, and related entities." .

#################################################################
#    Classes
#################################################################

# Main Classes
game:Game rdf:type owl:Class ;
    rdfs:label "Game" ;
    rdfs:comment "A video game" .

game:GameSeries rdf:type owl:Class ;
    rdfs:label "Game Series" ;
    rdfs:comment "A series of related games" .

game:Platform rdf:type owl:Class ;
    rdfs:label "Platform" ;
    rdfs:comment "A hardware or software platform on which games can be played" .

game:GameType rdf:type owl:Class ;
    rdfs:label "Game Type" ;
    rdfs:comment "The type of gameplay a game offers" .

game:Genre rdf:type owl:Class ;
    rdfs:label "Genre" ;
    rdfs:comment "A category of games characterized by a particular style, form, or content" .

game:Person rdf:type owl:Class ;
    rdfs:label "Person" ;
    rdfs:comment "A person involved in games as developer, player, etc." .

game:Publisher rdf:type owl:Class ;
    rdfs:label "Publisher" ;
    rdfs:comment "An entity that publishes games" .

game:Character rdf:type owl:Class ;
    rdfs:label "Character" ;
    rdfs:comment "A character that appears in a game" .

game:Product rdf:type owl:Class ;
    rdfs:label "Game Product/DLC" ;
    rdfs:comment "Additional content or products related to a game" .

game:Rating rdf:type owl:Class ;
    rdfs:label "Rating" ;
    rdfs:comment "Evaluation metrics for a game" .

game:Company rdf:type owl:Class ;
    rdfs:label "Company" ;
    rdfs:comment "A business entity that creates or publishes games" .

game:Country rdf:type owl:Class ;
    rdfs:label "Country" ;
    rdfs:comment "A country where a company is based" .

game:Event rdf:type owl:Class ;
    rdfs:label "Event" ;
    rdfs:comment "An organized gaming event or competition" .

game:Team rdf:type owl:Class ;
    rdfs:label "Team" ;
    rdfs:comment "A group of players who compete together" .

game:Record rdf:type owl:Class ;
    rdfs:label "Record" ;
    rdfs:comment "A notable achievement or statistic in a game or event" .

game:Prize rdf:type owl:Class ;
    rdfs:label "Prize" ;
    rdfs:comment "A reward given in a gaming competition" .

game:Match rdf:type owl:Class ;
    rdfs:label "Match" ;
    rdfs:comment "A single instance of competitive gameplay" .

# Person Subclasses
game:Developer rdf:type owl:Class ;
    rdfs:subClassOf game:Person ;
    rdfs:label "Developer" ;
    rdfs:comment "A person who develops games" .

game:ArtDirector rdf:type owl:Class ;
    rdfs:subClassOf game:Person ;
    rdfs:label "Art Director" ;
    rdfs:comment "A person who directs the artistic aspects of a game" .

game:Composer rdf:type owl:Class ;
    rdfs:subClassOf game:Person ;
    rdfs:label "Composer" ;
    rdfs:comment "A person who composes music for games" .

game:VoiceActor rdf:type owl:Class ;
    rdfs:subClassOf game:Person ;
    rdfs:label "Voice Actor" ;
    rdfs:comment "A person who provides voice acting for game characters" .

game:Player rdf:type owl:Class ;
    rdfs:subClassOf game:Person ;
    rdfs:label "Player" ;
    rdfs:comment "A person who plays games" .

# GameType Subclasses
game:SinglePlayer rdf:type owl:Class ;
    rdfs:subClassOf game:GameType ;
    rdfs:label "Single Player" ;
    rdfs:comment "A game designed for one player" .

game:Multiplayer rdf:type owl:Class ;
    rdfs:subClassOf game:GameType ;
    rdfs:label "Multiplayer" ;
    rdfs:comment "A game designed for multiple players" .

game:Competitive rdf:type owl:Class ;
    rdfs:subClassOf game:GameType ;
    rdfs:label "Competitive" ;
    rdfs:comment "A game designed for competitive play" .

# Genre Subclasses
game:RPG rdf:type owl:Class ;
    rdfs:subClassOf game:Genre ;
    rdfs:label "RPG" ;
    rdfs:comment "Role-playing game genre" .

game:MMORPG rdf:type owl:Class ;
    rdfs:subClassOf game:RPG ;
    rdfs:label "MMORPG" ;
    rdfs:comment "Massively multiplayer online role-playing game genre" .

game:FPS rdf:type owl:Class ;
    rdfs:subClassOf game:Genre ;
    rdfs:label "FPS" ;
    rdfs:comment "First-person shooter genre" .

game:Strategy rdf:type owl:Class ;
    rdfs:subClassOf game:Genre ;
    rdfs:label "Strategy" ;
    rdfs:comment "Strategy game genre" .

# Product Subclasses
game:Skin rdf:type owl:Class ;
    rdfs:subClassOf game:Product ;
    rdfs:label "Skin" ;
    rdfs:comment "Cosmetic item that changes a character's appearance" .

game:Item rdf:type owl:Class ;
    rdfs:subClassOf game:Product ;
    rdfs:label "Item" ;
    rdfs:comment "An in-game item available as additional content" .

game:Expansion rdf:type owl:Class ;
    rdfs:subClassOf game:Product ;
    rdfs:label "Expansion" ;
    rdfs:comment "Significant additional content that expands a game" .

# Event Subclasses
game:Competition rdf:type owl:Class ;
    rdfs:subClassOf game:Event ;
    rdfs:label "Competition" ;
    rdfs:comment "An organized gaming competition" .

game:Leaderboard rdf:type owl:Class ;
    rdfs:subClassOf game:Event ;
    rdfs:label "Leaderboard" ;
    rdfs:comment "A ranked listing of players or teams" .

#################################################################
#    Object Properties
#################################################################

game:hasTitle rdf:type owl:ObjectProperty ;
    rdfs:domain game:Game ;
    rdfs:range xsd:string ;
    rdfs:label "has title" ;
    rdfs:comment "The title of a game" .

game:hasDescription rdf:type owl:ObjectProperty ;
    rdfs:domain game:Game ;
    rdfs:range xsd:string ;
    rdfs:label "has description" ;
    rdfs:comment "The description of a game" .

game:hasReleaseDate rdf:type owl:ObjectProperty ;
    rdfs:domain game:Game ;
    rdfs:range xsd:date ;
    rdfs:label "has release date" ;
    rdfs:comment "The release date of a game" .

game:hasGameURL rdf:type owl:ObjectProperty ;
    rdfs:domain game:Game ;
    rdfs:range xsd:anyURI ;
    rdfs:label "has game URL" ;
    rdfs:comment "The URL associated with a game" .

game:hasRating rdf:type owl:ObjectProperty ;
    rdfs:domain game:Game ;
    rdfs:range game:Rating ;
    rdfs:label "has rating" ;
    rdfs:comment "The rating of a game" .

game:hasProduct rdf:type owl:ObjectProperty ;
    rdfs:domain game:Game ;
    rdfs:range game:Product ;
    rdfs:label "has product" ;
    rdfs:comment "Additional content or products associated with a game" .

game:partOfSeries rdf:type owl:ObjectProperty ;
    rdfs:domain game:Game ;
    rdfs:range game:GameSeries ;
    rdfs:label "part of series" ;
    rdfs:comment "The game series a game belongs to" .

game:availableOnPlatform rdf:type owl:ObjectProperty ;
    rdfs:domain game:Game ;
    rdfs:range game:Platform ;
    rdfs:label "available on platform" ;
    rdfs:comment "The platform a game is available on" .

game:hasGameType rdf:type owl:ObjectProperty ;
    rdfs:domain game:Game ;
    rdfs:range game:GameType ;
    rdfs:label "has game type" ;
    rdfs:comment "The type of gameplay a game offers" .

game:hasGenre rdf:type owl:ObjectProperty ;
    rdfs:domain game:Game ;
    rdfs:range game:Genre ;
    rdfs:label "has genre" ;
    rdfs:comment "The genre of a game" .

game:developedBy rdf:type owl:ObjectProperty ;
    rdfs:domain game:Game ;
    rdfs:range game:Person ;
    rdfs:label "developed by" ;
    rdfs:comment "The person who developed a game" .

game:publishedBy rdf:type owl:ObjectProperty ;
    rdfs:domain game:Game ;
    rdfs:range game:Publisher ;
    rdfs:label "published by" ;
    rdfs:comment "The publisher of a game" .

game:featuresCharacter rdf:type owl:ObjectProperty ;
    rdfs:domain game:Game ;
    rdfs:range game:Character ;
    rdfs:label "features character" ;
    rdfs:comment "A character featured in a game" .

game:forSaleIn rdf:type owl:ObjectProperty ;
    rdfs:domain game:Product ;
    rdfs:range game:Game ;
    rdfs:label "for sale in" ;
    rdfs:comment "The game in which a product is available for sale" .

game:ownsCompany rdf:type owl:ObjectProperty ;
    rdfs:domain game:Publisher ;
    rdfs:range game:Company ;
    rdfs:label "owns company" ;
    rdfs:comment "A company owned by a publisher" .

game:hasCompanyTitle rdf:type owl:ObjectProperty ;
    rdfs:domain game:Company ;
    rdfs:range xsd:string ;
    rdfs:label "has company title" ;
    rdfs:comment "The title of a company" .

game:basedInCountry rdf:type owl:ObjectProperty ;
    rdfs:domain game:Company ;
    rdfs:range game:Country ;
    rdfs:label "based in country" ;
    rdfs:comment "The country a company is based in" .

game:workedOnGame rdf:type owl:ObjectProperty ;
    rdfs:domain [ rdf:type owl:Class ;
                 owl:unionOf ( game:Developer game:ArtDirector game:Composer game:VoiceActor )
               ] ;
    rdfs:range game:Game ;
    rdfs:label "worked on game" ;
    rdfs:comment "The game a person worked on" .

game:hasNumberOfRatings rdf:type owl:ObjectProperty ;
    rdfs:domain game:Rating ;
    rdfs:range xsd:positiveInteger ;
    rdfs:label "has number of ratings" ;
    rdfs:comment "The number of ratings a game has received" .

game:hasScore rdf:type owl:ObjectProperty ;
    rdfs:domain game:Rating ;
    rdfs:range xsd:decimal ;
    rdfs:label "has score" ;
    rdfs:comment "The score of a game's rating" .

game:hasCompletionPercentage rdf:type owl:ObjectProperty ;
    rdfs:domain game:Rating ;
    rdfs:range xsd:decimal ;
    rdfs:label "has completion percentage" ;
    rdfs:comment "The percentage of players who completed a game" .

game:hasCharacterName rdf:type owl:ObjectProperty ;
    rdfs:domain game:Character ;
    rdfs:range xsd:string ;
    rdfs:label "has character name" ;
    rdfs:comment "The name of a character" .

game:hasRecord rdf:type owl:ObjectProperty ;
    rdfs:domain game:Event ;
    rdfs:range game:Record ;
    rdfs:label "has record" ;
    rdfs:comment "A record associated with an event" .

game:hasPrize rdf:type owl:ObjectProperty ;
    rdfs:domain game:Event ;
    rdfs:range game:Prize ;
    rdfs:label "has prize" ;
    rdfs:comment "A prize offered in an event" .

game:hasEventScore rdf:type owl:ObjectProperty ;
    rdfs:domain game:Event ;
    rdfs:range xsd:decimal ;
    rdfs:label "has event score" ;
    rdfs:comment "The score in an event" .

game:hasMatch rdf:type owl:ObjectProperty ;
    rdfs:domain game:Event ;
    rdfs:range game:Match ;
    rdfs:label "has match" ;
    rdfs:comment "A match that is part of an event" .

game:competesInEvent rdf:type owl:ObjectProperty ;
    rdfs:domain game:Player ;
    rdfs:range game:Event ;
    rdfs:label "competes in event" ;
    rdfs:comment "An event a player competes in" .

game:memberOfTeam rdf:type owl:ObjectProperty ;
    rdfs:domain game:Player ;
    rdfs:range game:Team ;
    rdfs:label "member of team" ;
    rdfs:comment "The team a player is a member of" .

game:wonEvent rdf:type owl:ObjectProperty ;
    rdfs:domain game:Team ;
    rdfs:range game:Event ;
    rdfs:label "won event" ;
    rdfs:comment "An event won by a team" .

game:lostEvent rdf:type owl:ObjectProperty ;
    rdfs:domain game:Team ;
    rdfs:range game:Event ;
    rdfs:label "lost event" ;
    rdfs:comment "An event lost by a team" .

#################################################################
#    Data Properties
#################################################################

game:title rdf:type owl:DatatypeProperty ;
    rdfs:domain game:Game ;
    rdfs:range xsd:string ;
    rdfs:label "title" ;
    rdfs:comment "The title of a game" .

game:description rdf:type owl:DatatypeProperty ;
    rdfs:domain game:Game ;
    rdfs:range xsd:string ;
    rdfs:label "description" ;
    rdfs:comment "The description of a game" .

game:releaseDate rdf:type owl:DatatypeProperty ;
    rdfs:domain game:Game ;
    rdfs:range xsd:date ;
    rdfs:label "release date" ;
    rdfs:comment "The release date of a game" .

game:gameURL rdf:type owl:DatatypeProperty ;
    rdfs:domain game:Game ;
    rdfs:range xsd:anyURI ;
    rdfs:label "game URL" ;
    rdfs:comment "The URL associated with a game" .

game:companyTitle rdf:type owl:DatatypeProperty ;
    rdfs:domain game:Company ;
    rdfs:range xsd:string ;
    rdfs:label "company title" ;
    rdfs:comment "The title of a company" .

game:numberOfRatings rdf:type owl:DatatypeProperty ;
    rdfs:domain game:Rating ;
    rdfs:range xsd:positiveInteger ;
    rdfs:label "number of ratings" ;
    rdfs:comment "The number of ratings a game has received" .

game:score rdf:type owl:DatatypeProperty ;
    rdfs:domain game:Rating ;
    rdfs:range xsd:decimal ;
    rdfs:label "score" ;
    rdfs:comment "The score of a game's rating" .

game:completionPercentage rdf:type owl:DatatypeProperty ;
    rdfs:domain game:Rating ;
    rdfs:range xsd:decimal ;
    rdfs:label "completion percentage" ;
    rdfs:comment "The percentage of players who completed a game" .

game:characterName rdf:type owl:DatatypeProperty ;
    rdfs:domain game:Character ;
    rdfs:range xsd:string ;
    rdfs:label "character name" ;
    rdfs:comment "The name of a character" .

game:eventScore rdf:type owl:DatatypeProperty ;
    rdfs:domain game:Event ;
    rdfs:range xsd:decimal ;
    rdfs:label "event score" ;
    rdfs:comment "The score in an event" .

# Additional assertions and axioms

# Define some disjoint classes
game:Person owl:disjointWith game:Game, game:Character, game:Company .
game:Game owl:disjointWith game:Event, game:Character, game:Team .
game:Product owl:disjointWith game:Person, game:Event, game:Company .

# Define some inverse properties
game:developedBy owl:inverseOf game:workedOnGame .
game:memberOfTeam owl:inverseOf [ rdf:type owl:ObjectProperty ;
                                 rdfs:label "has member" ;
                                 rdfs:domain game:Team ;
                                 rdfs:range game:Player ] .

# Some functional properties (can only have one value)
game:hasTitle rdf:type owl:FunctionalProperty .
game:hasReleaseDate rdf:type owl:FunctionalProperty .
game:basedInCountry rdf:type owl:FunctionalProperty .